# NCBI API Key (optional)
VITE_NCBI_API_KEY=your_api_key_here

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRIES=3
VITE_API_CACHE_TTL=3600000

# Proxy Configuration
VITE_PROXY_ENABLED=true
VITE_PROXY_PORT=3001

# Batch Processing Configuration
VITE_MAX_BATCH_SIZE=200
VITE_MAX_CONCURRENT_REQUESTS=5

# Data Processing Configuration
VITE_NORMALIZATION_METHOD=quantile
VITE_LOG_TRANSFORM_ENABLED=true
VITE_TRIM_OUTLIERS=true

# Note: Copy this file to .env and fill in your actual values
